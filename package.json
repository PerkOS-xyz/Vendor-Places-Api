    "test:full-integration": "tsx tests/client-full-integration-test.ts",
    "test:full-integration:remote": "API_BASE_URL=https://places-api.x402hub.xyz tsx tests/client-full-integration-test.ts",
    "test:production": "tsx tests/production-api-test.ts",
