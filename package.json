    "@coinbase/x402": "^0.6.5",
    "@googlemaps/google-maps-services-js": "^3.3.42",
    "@modelcontextprotocol/sdk": "^1.18.2",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "x402-express": "^0.6.5",
    "x402-fetch": "^0.6.0"
