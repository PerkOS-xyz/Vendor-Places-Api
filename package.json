    "test:mcp-simple": "tsx tests/test-mcp-simple.ts",
    "test:mcp-manual": "tsx tests/test-mcp-manual.ts",
    "test:mcp-direct": "tsx tests/test-mcp-direct.ts",
    "test:mainnet-payment": "tsx tests/test-mainnet-payment.ts",
    "test:production-payment": "tsx tests/test-production-payment.ts",
    "test:real-payment": "tsx tests/test-real-payment.ts",
    "test:x402-local": "tsx tests/test-x402-local.ts",
    "test:complex-query": "tsx tests/test-complex-query.ts",
